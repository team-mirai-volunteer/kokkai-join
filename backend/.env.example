DATABASE_URL=postgresql://kokkai_user:kokkai_pass@localhost:5432/kokkai_db

# Embedding Provider Configuration
# Provider type: ollama (default) or novita
EMBEDDING_PROVIDER=novita

# Novita configuration (when EMBEDDING_PROVIDER=novita) - OpenAI compatible
EMBEDDING_API_KEY=
EMBEDDING_MODEL=baai/bge-m3
EMBEDDING_BASE_URL=https://api.novita.ai/openai

# Ollama configuration (when EMBEDDING_PROVIDER=ollama)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_EMBEDDING_MODEL=bge-m3

# LLM configuration
LLM_MODEL=gpt-4o-mini
# LLM_MODEL_QUERY_PLANNING=
# LLM_MODEL_RELEVANCE=
# LLM_MODEL_ANSWER=
# LLM_MODEL_SECTION=
# OPENAI_MODEL=

# from https://platform.openai.com/api-keys
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5

KOKKAI_RAG_URL=http://localhost:8001/v1/search
